<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- The bottom layer is blue square.-->
    <item
        android:left="0dp"
        android:top="0dp"
        android:right="0dp"
        android:bottom="0dp">
        <shape>
            <solid android:color="#F08801" />
            <size android:height="108dp"
                android:width="108dp" />
            <corners android:radius="20dp"/>
        </shape>
    </item>

    <item
        android:left="0dp"
        android:top="21dp"
        android:right="0dp"
        android:bottom="21dp">
        <shape>
            <solid android:color="#FFFFFF" />
        </shape>
    </item>

</layer-list>